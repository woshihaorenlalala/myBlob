<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.blob.module.module.blob.dao.TBlobDao">
    <select id="findList">
      SELECT
            id,
            title,
            content,
            `show`,
            create_time,
            delflag
        FROM
            t_blob
    </select>
</mapper>
